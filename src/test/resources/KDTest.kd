# =============================================================================
# ğŸ“ COORDINATE LITERALS
# =============================================================================

coordinates {
    # Standard notation (zero-based x, y)
    origin .coordinate(x=0, y=0)
    standard_10_20 .coordinate(x=10, y=20)
    large_coords .coordinate(x=100, y=500)

    # Sheet notation (letter column, one-based row)
    cell_A1 .coordinate(c="A", r=1)
    cell_E8 .coordinate(c="E", r=8)
    cell_Z26 .coordinate(c="Z", r=26)

    # Multi-letter columns
    cell_AA1 .coordinate(c="AA", r=1)
    cell_AA100 .coordinate(c="AA", r=100)
    cell_CW51 .coordinate(c="CW", r=51)

    # With z component (3D)
    coord_3d .coordinate(x=1, y=2, z=5)
    coord_3d_sheet .coordinate(c="B", r=3, z=10)

    # As attributes
    cell value=42 position=.coordinate(x=3, y=5)
    selection start=.coordinate(c="A", r=1) end=.coordinate(c="E", r=10)

    # In lists
    path [.coordinate(x=0, y=0), .coordinate(x=1, y=1), .coordinate(x=2, y=2)]

    # Equivalent coordinates (same position, different notation)
    equivalents {
        standard .coordinate(x=4, y=7)
        sheet .coordinate(c="E", r=8)
